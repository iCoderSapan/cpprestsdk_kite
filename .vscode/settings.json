{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/vcpkg/**": true,
        "build.x86.debug": true,
        "build.x86.release": true,
        "build.x64.debug": true,
        "build.x64.release": true,
        "out": true,
    },
    "cSpell.words": [
        "XPLATSTR",
        "blittable",
        "pplx",
        "rdpos",
        "rgpsz"
    ],
    "files.associations": {
        "atomic": "cpp",
        "*.dat": "cpp",
        "ostream": "cpp",
        "iostream": "cpp",
        "*.ipp": "cpp",
        "condition_variable": "cpp",
        "future": "cpp",
        "*.tcc": "cpp",
        "array": "cpp",
        "memory": "cpp",
        "istream": "cpp",
        "functional": "cpp",
        "tuple": "cpp",
        "utility": "cpp",
        "variant": "cpp",
        "strstream": "cpp",
        "bitset": "cpp",
        "random": "cpp",
        "new": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "type_traits": "cpp",
        "iosfwd": "cpp",
        "stdexcept": "cpp"
    }
}
